<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidpracticeforconcepclearing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cvd"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cvd"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.StateList"></activity>
        <activity android:name=".View.StateDetails" />
        <activity android:name=".View.DetailActivity" />
        <activity android:name=".View.AffectedCountries" />
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>